{
    "key": "shopify/inventory/tag_fulfillable_orders_kj/tag",
    "name": "Tag Fulfillable Orders",
    "version": "1.0.0",
    "description": "",
    "seconds": 135,
    "enabled": false,
    "setup": true,
    "config": {
        "inputs": [
            {
                "schema": 2,
                "trigger_type": "input",
                "type": "webhook",
                "name": "Webhook Received",
                "key": "webhook",
                "metadata": [],
                "local_fields": [],
                "selected_fields": [
                    "host"
                ],
                "on_error": "default",
                "weight": 0
            }
        ],
        "outputs": [
            {
                "schema": 3,
                "trigger_type": "output",
                "type": "shopify",
                "entity": "order",
                "action": "list",
                "name": "Get List of Orders",
                "key": "shopify",
                "operation_id": "get_orders",
                "metadata": {
                    "api_endpoint": "get admin/orders.json",
                    "query": {
                        "limit": "{{ template | label: 'How many orders to process at once?', description: 'We recommend setting this to something low initially to run a test, and then setting to 250 when you are ready to make it live.', default: 10 }}",
                        "status": "open",
                        "fulfillment_status": "unfulfilled"
                    }
                },
                "local_fields": [],
                "selected_fields": [
                    "query.status",
                    "query.limit",
                    "query.fulfillment_status"
                ],
                "on_error": "default",
                "weight": 0
            },
            {
                "schema": 5.1,
                "trigger_type": "output",
                "type": "loop",
                "entity": "loop",
                "name": "Loop Over Orders",
                "version": "v2",
                "key": "loop",
                "operation_id": "loop_loop",
                "metadata": {
                    "key": "{{shopify}}",
                    "filter": {
                        "comparison": "equals"
                    }
                },
                "local_fields": [],
                "selected_fields": [
                    "key",
                    "filter",
                    "filter.a",
                    "filter.comparison",
                    "filter.b"
                ],
                "on_error": "default",
                "weight": 1
            },
            {
                "schema": 4,
                "trigger_type": "output",
                "type": "data",
                "version": "v1",
                "entity": "record",
                "action": "query",
                "name": "Lookup Order In Assigned Inventory",
                "key": "data_2",
                "operation_id": "get_database_table",
                "metadata": {
                    "api_endpoint": "get /{database}/{table}",
                    "table": "assigned_inventory",
                    "where_clause": {
                        "comparison": "equals",
                        "b": "{{loop.id}}",
                        "a": "order_id"
                    }
                },
                "local_fields": [],
                "selected_fields": [
                    "table",
                    "where_clause",
                    "where_clause.a",
                    "where_clause.comparison",
                    "where_clause.b"
                ],
                "on_error": "default",
                "weight": 2
            },
            {
                "schema": 4.1,
                "trigger_type": "output",
                "type": "filter",
                "name": "If Order Hasn't Been Assigned Yet",
                "key": "filter_1",
                "metadata": {
                    "a": "{{data_2.0.mesa_id}}",
                    "comparison": "is empty",
                    "additional": [
                        {
                            "operator": "and",
                            "comparison": "equals"
                        }
                    ]
                },
                "local_fields": [],
                "selected_fields": [
                    "a",
                    "comparison",
                    "additional",
                    "additional[].operator",
                    "additional[].a",
                    "additional[].comparison",
                    "additional[].b"
                ],
                "on_error": "default",
                "weight": 3
            },
            {
                "schema": 4,
                "trigger_type": "output",
                "type": "data",
                "version": "v1",
                "entity": "record",
                "action": "query",
                "name": "Get Assigned Inventory By Variant",
                "key": "data_1",
                "operation_id": "get_database_table",
                "metadata": {
                    "api_endpoint": "get /{database}/{table}",
                    "query": "{% assign variant_ids = \"\" %} \n\n{% for item in loop.line_items %}\n  {% if forloop.first %}\n    {% assign variant_ids = variant_ids | append: \"'\" | append: item.variant_id | append: \"'\" %}\n  {% else %}\n    {% assign variant_ids = variant_ids | append: \", '\" | append: item.variant_id | append: \"'\" %}\n  {% endif %}\n{% endfor %}\n\nSELECT variant_id, SUM(quantity) AS quantity FROM \"assigned_inventory\" \nWHERE variant_id IN ({{ variant_ids }})\nGROUP BY variant_id",
                    "table": "assigned_inventory",
                    "where_clause": {
                        "comparison": "equals"
                    }
                },
                "local_fields": [],
                "selected_fields": [
                    "query"
                ],
                "on_error": "default",
                "weight": 4
            },
            {
                "schema": 2,
                "trigger_type": "output",
                "type": "custom",
                "name": "See If All Variants Are Fulfillable",
                "key": "custom",
                "metadata": {
                    "script": "custom.js"
                },
                "local_fields": [],
                "selected_fields": [
                    "script"
                ],
                "on_error": "default",
                "weight": 5
            },
            {
                "schema": 4.1,
                "trigger_type": "output",
                "type": "filter",
                "name": "If Fulfillable",
                "key": "filter",
                "metadata": {
                    "a": "{{custom.is_fulfillable}}",
                    "comparison": "equals",
                    "b": "1",
                    "additional": [
                        {
                            "operator": "and",
                            "comparison": "equals"
                        }
                    ]
                },
                "local_fields": [],
                "selected_fields": [
                    "a",
                    "comparison",
                    "b",
                    "additional",
                    "additional[].operator",
                    "additional[].a",
                    "additional[].comparison",
                    "additional[].b"
                ],
                "on_error": "default",
                "weight": 6
            },
            {
                "schema": 3,
                "trigger_type": "output",
                "type": "shopify",
                "entity": "order",
                "action": "tag_add",
                "name": "Order Add Tag",
                "key": "shopify_3",
                "operation_id": "post_mesa_orders_order_id_tag",
                "metadata": {
                    "api_endpoint": "post mesa/orders/{{order_id}}/tag.json",
                    "order_id": "{{loop.id}}",
                    "body": {
                        "tag": "Fulfillable"
                    }
                },
                "local_fields": [],
                "selected_fields": [
                    "order_id",
                    "body",
                    "body.tag"
                ],
                "on_error": "default",
                "weight": 7
            },
            {
                "schema": 2,
                "trigger_type": "output",
                "type": "custom",
                "name": "Assign Inventory In Database",
                "key": "custom_1",
                "metadata": {
                    "script": "custom_1.js"
                },
                "local_fields": [],
                "selected_fields": [
                    "script"
                ],
                "on_error": "default",
                "weight": 8
            }
        ]
    }
}