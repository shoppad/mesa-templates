{
  "key": "shopify/inventory/import_delta_from_other_store_kj",
  "name": "Import Inventory Delta From Other Store",
  "version": "1.0.0",
  "description": "",
  "seconds": 135,
  "enabled": false,
  "setup": true,
  "config": {
    "inputs": [
      {
        "schema": 2,
        "trigger_type": "input",
        "type": "webhook",
        "name": "Webhook Received",
        "key": "webhook",
        "metadata": [],
        "local_fields": [],
        "selected_fields": [],
        "on_error": "default",
        "weight": 0
      }
    ],
    "outputs": [
      {
        "schema": 3,
        "trigger_type": "output",
        "type": "shopify",
        "entity": "product_variant",
        "action": "sku_to_product_variant",
        "name": "Retrieve Product Variant Details from SKU",
        "key": "shopify",
        "operation_id": "get_mesa_productvariants_product_sku_sku_lookup",
        "metadata": {
          "api_endpoint": "get mesa/productVariants/{{product_sku}}/sku-lookup.json",
          "product_sku": "{{webhook.sku}}"
        },
        "local_fields": [],
        "selected_fields": [
          "product_sku"
        ],
        "on_error": "ignore",
        "weight": 0
      },
      {
        "schema": 3,
        "trigger_type": "output",
        "type": "shopify",
        "entity": "location",
        "action": "list",
        "name": "Get List of Locations",
        "key": "shopify_1",
        "operation_id": "get_locations",
        "metadata": {
          "api_endpoint": "get admin/locations.json",
          "parameters": "name=Mesa%20HQ"
        },
        "local_fields": [],
        "selected_fields": [
          "parameters"
        ],
        "on_error": "default",
        "weight": 1
      },
      {
        "schema": 5.1,
        "trigger_type": "output",
        "type": "loop",
        "entity": "map",
        "name": "Get Location By Name",
        "version": "v2",
        "key": "loop",
        "operation_id": "loop_map",
        "metadata": {
          "key": "{{shopify_1}}",
          "map_field": "{{shopify_1[].id}}",
          "filter": {
            "a": "{{shopify_1[].name}}",
            "comparison": "equals",
            "b": "{{webhook.location}}"
          }
        },
        "local_fields": [],
        "selected_fields": [
          "filter.a",
          "filter.comparison",
          "filter.b"
        ],
        "on_error": "default",
        "weight": 2
      },
      {
        "schema": 3,
        "trigger_type": "output",
        "type": "shopify",
        "entity": "inventory_level",
        "action": "adjust",
        "name": "Adjust Inventory Level",
        "key": "shopify_2",
        "operation_id": "post_inventory_levels_adjust",
        "metadata": {
          "api_endpoint": "post admin/inventory_levels/adjust.json",
          "body": {
            "location_id": "{{loop.items.0}}",
            "inventory_item_id": "{{shopify.inventory_item_id}}",
            "available_adjustment": "{{webhook.delta}}"
          }
        },
        "local_fields": [],
        "selected_fields": [
          "body",
          "body.location_id",
          "body.inventory_item_id",
          "body.available_adjustment"
        ],
        "on_error": "default",
        "weight": 3
      },
      {
        "schema": 4,
        "trigger_type": "output",
        "type": "data",
        "entity": "record",
        "action": "create",
        "name": "Create Record",
        "key": "data",
        "operation_id": "post_database_table",
        "metadata": {
          "api_endpoint": "post /{database}/{table}",
          "create": "existing",
          "columns": [
            {
              "key": "available",
              "type": "numeric",
              "value": "{{shopify_2.available}}",
              "disabled": "65f9bcc87cc915a57b0d0df5"
            },
            {
              "key": "level_updated_at",
              "type": "timestamptz",
              "value": null,
              "disabled": "65f9bcc87cc915a57b0d0df5"
            },
            {
              "key": "delta",
              "type": "numeric",
              "value": "{{webhook.delta}}",
              "disabled": "65f9bcc87cc915a57b0d0df5"
            },
            {
              "key": "sku",
              "type": "varchar",
              "value": "{{webhook.sku}}",
              "disabled": "65f9bcc87cc915a57b0d0df5"
            },
            {
              "key": "status",
              "type": "varchar",
              "value": "Applied From Store A",
              "disabled": "65f9bcc87cc915a57b0d0df5"
            },
            {
              "key": "run_time",
              "type": "varchar",
              "value": "{{webhook.run_time}}",
              "disabled": "65f9bcc87cc915a57b0d0df5"
            },
            {
              "key": "location_name",
              "type": "varchar",
              "value": "{{webhook.location}}",
              "disabled": "65f9bcc87cc915a57b0d0df5"
            },
            {
              "key": "product_id",
              "type": "varchar",
              "value": null,
              "disabled": "65f9bcc87cc915a57b0d0df5"
            },
            {
              "key": "variant_id",
              "type": "varchar",
              "value": null,
              "disabled": "65f9bcc87cc915a57b0d0df5"
            }
          ],
          "table": "inventory_levels"
        },
        "local_fields": [],
        "selected_fields": [
          "create",
          "table",
          "columns",
          "columns[].key",
          "columns[].type",
          "columns[].value",
          "columns[].disabled"
        ],
        "on_error": "default",
        "weight": 4
      }
    ]
  }
}