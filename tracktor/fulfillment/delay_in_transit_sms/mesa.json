{
    "key": "tracktor/fulfillment/delay_in_transit_sms",
    "name": "Get SMS Alerts for Delayed Packages in Transit",
    "version": "1.0.0",
    "enabled": false,
    "setup": true,
    "config": {
        "inputs": [
            {
                "schema": 2,
                "trigger_type": "input",
                "type": "tracktor",
                "entity": "fulfillment",
                "action": "fulfillment/in_transit",
                "name": "Fulfillment Status is In Transit",
                "key": "tracktor",
                "operation_id": "fulfillment_in_transit",
                "metadata": [],
                "selected_fields": [],
                "on_error": "default",
                "weight": 0
            }
        ],
        "outputs": [
            {
                "schema": 2,
                "trigger_type": "output",
                "type": "delay",
                "name": "Delay",
                "version": "v2",
                "key": "delay",
                "operation_id": "delay",
                "metadata": {
                    "amount": "60",
                    "unit": "hours",
                    "test_bypass": false
                },
                "local_fields": [],
                "selected_fields": [],
                "on_error": "default",
                "weight": 0
            },
            {
                "schema": 3,
                "trigger_type": "output",
                "type": "tracktor",
                "entity": "fulfillment",
                "action": "retrieve",
                "name": "Retrieve Fulfillment",
                "key": "tracktor_1",
                "operation_id": "RetrievethetrackingstatusofaFulfillment",
                "metadata": {
                    "api_endpoint": "get \/fulfillments\/{fulfillment_id}.json",
                    "fulfillment_id": "{{tracktor.fulfillment_id}}"
                },
                "local_fields": [],
                "selected_fields": [],
                "on_error": "default",
                "weight": 1
            },
            {
                "schema": 4.1,
                "trigger_type": "output",
                "type": "filter",
                "name": "Filter",
                "key": "filter",
                "operation_id": "filter",
                "metadata": {
                    "a": "{{tracktor_1.latest_status.label}}",
                    "comparison": "does not equal",
                    "b": "{{tracktor_1.status.delivered.label}}",
                    "additional": [
                        {
                            "operator": "and",
                            "comparison": "equals"
                        }
                    ]
                },
                "local_fields": [],
                "selected_fields": [],
                "on_error": "default",
                "weight": 2
            },
            {
                "schema": 3.1,
                "trigger_type": "output",
                "type": "shopify",
                "entity": "shop",
                "action": "list",
                "name": "Retrieve Shop",
                "key": "shopify",
                "operation_id": "get_shop",
                "metadata": {
                    "api_endpoint": "get admin\/shop.json"
                },
                "local_fields": [],
                "selected_fields": [],
                "on_error": "default",
                "weight": 3
            },
            {
                "schema": 2,
                "trigger_type": "output",
                "type": "sms",
                "entity": "message",
                "action": "send",
                "name": "Send Message",
                "key": "sms",
                "operation_id": "message_send",
                "metadata": {
                    "to": "{{shopify.phone}}",
                    "message": "Order {{tracktor_1.order_name}} has been in transit for over 60 hours!"
                },
                "local_fields": [],
                "selected_fields": [],
                "on_error": "default",
                "weight": 4
            }
        ]
    }
}